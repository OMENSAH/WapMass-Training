class Car {
  //properties
  String name;
  String color;
  String manufacturer;
  int year_made;
  double speed = 200.30;
  
  
  //methods
  double accelerate() {
    speed = speed +10;
    return speed;
  }
  
  void changeColor(String newColor){
    color = newColor;
  }
  
  public static void main(String[] args){
    //  now we can create objects from our car; The process is called instantiation. 
    // Creating a new car called BMW
    
    Car bmw = new Car();
    // bmw as object of car  now has access to all the properties and methods of car class
    
    // Access properties and methods with dot operator.
    System.out.println("Color:" + bmw.color);
    System.out.println("name:" + bmw.name);
    System.out.println("Speed:" + bmw.speed);
    
    
  }
}
  
  